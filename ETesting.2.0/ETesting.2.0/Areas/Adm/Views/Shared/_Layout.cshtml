<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title - ETesting</title>
    <meta name="keywords" content="HTML5 Bootstrap 3 Admin Template UI Theme" />
    <meta name="description" content="AdminDesigns - A Responsive HTML5 Admin UI Framework">
    <meta name="author" content="AdminDesigns">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Font CSS (Via CDN) -->
    <link rel="stylesheet" type="text/css" href="~/assets/fonts/fonts.googleapis.com/css.css">

    <!-- Theme CSS -->
    <link rel="stylesheet" type="text/css" href="~/assets/skin/default_skin/css/theme.css">
    <link rel="stylesheet" type="text/css" href="~/assets/skin/default_skin/css/theme2.css">
    <link rel="stylesheet" type="text/css" href="~/assets/skin/default_skin/css/theme3.css">

    <!-- Admin Forms CSS -->
    <link rel="stylesheet" type="text/css" href="~/assets/admin-tools/admin-forms/css/admin-forms.css">

    <!-- Favicon -->
    <link rel="shortcut icon" href="~/assets/img/favicon.ico">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
      <![endif]-->

    @RenderSection("pluginStyles", required: false)
</head>
<body class="dashboard-page sb-l-o sb-r-c">
    <!-------------------------------------------------------------+
      <body> Helper Classes:
    ---------------------------------------------------------------+
      '.sb-l-o' - Sets Left Sidebar to "open"
      '.sb-l-m' - Sets Left Sidebar to "minified"
      '.sb-l-c' - Sets Left Sidebar to "closed"

      '.sb-r-o' - Sets Right Sidebar to "open"
      '.sb-r-c' - Sets Right Sidebar to "closed"
    ---------------------------------------------------------------+
     Example: <body class="example-page sb-l-o sb-r-c">
     Results: Sidebar left Open, Sidebar right Closed
    --------------------------------------------------------------->
    <!-- For Demo Purposes - Theme Settings Pane -->
    @*@Html.Partial("_ThemeSettings")*@
    <!-- End: Theme Settings Pane -->
    <!-- Start: Main -->
    <div id="main">
        <!-- Start: Header -->
        @Html.Partial("_Header")
        <!-- End: Header -->
        <!-- Start: Sidebar Left -->
        @Html.Partial("_SidebarLeft")
        <!-- End: Sidebar Left -->
        <!-- Start: Content-Wrapper -->
        <section id="content_wrapper">
            <!-- Start: Topbar-Dropdown -->
            @Html.Partial("_TopbarDropdown")
            <!-- End: Topbar-Dropdown -->
            <!-- Start: Topbar -->
            @Html.Partial("_Topbar")
            <!-- End: Topbar -->
            <!-- Begin: Content -->
            <section id="content" class="animated fadeIn">
                @RenderBody()
            </section>
            <!-- End: Content -->
            <!-- Begin: Page Footer -->
            @Html.Partial("_Footer")
            <!-- End: Page Footer -->
        </section>
        <!-- End: Content-Wrapper -->
        <!-- Start: Right Sidebar -->
        @Html.Partial("_SidebarRight")
        <!-- End: Right Sidebar -->

    </div>
    <!-- End: Main -->
    <!-- BEGIN: PAGE SCRIPTS -->
    <!-- jQuery -->
    <script src="~/Scripts/jquery-3.1.1.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.10.4.min.js"></script>

    <!-- HighCharts Plugin -->
    <script src="~/Scripts/plugins/highcharts/highcharts.js"></script>

    <!-- Sparklines Plugin -->
    <script src="~/Scripts/plugins/sparkline/jquery.sparkline.min.js"></script>

    <!-- Simple Circles Plugin -->
    <script src="~/Scripts/plugins/circles/circles.js"></script>

    <!-- JvectorMap Plugin + US Map (more maps in plugin/assets folder) -->
    <script src="~/Scripts/plugins/jvectormap/jquery.jvectormap.min.js"></script>
    <script src="~/Scripts/plugins/jvectormap/assets/jquery-jvectormap-us-lcc-en.js"></script>

    <!-- Theme Javascript -->
    <script src="~/assets/js/utility/utility.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>

    <script src="~/assets/js/demo/demo.js"></script>
    <script src="~/assets/js/main.js"></script>

    <!-- Widget Javascript -->
    <script src="~/assets/js/demo/widgets.js"></script>
    <script src="~/assets/js/init.js"></script>

    @RenderSection("pluginScripts", required: false)
    @RenderSection("scripts", required: false)
    <!-- END: PAGE SCRIPTS -->
</body>
</html>